<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="parent" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicTacToe">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" percentWidth="33.33333" prefWidth="100.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="200" percentWidth="33.33333" prefWidth="100.0" />
        <ColumnConstraints hgrow="ALWAYS" minWidth="200.0" percentWidth="33.33333" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="200.0" percentHeight="27" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="200.0" percentHeight="27" prefHeight="30.0" vgrow="ALWAYS" />
        <RowConstraints minHeight="200.0" percentHeight="27" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints minHeight="200.0" percentHeight="19" prefHeight="30.0" vgrow="ALWAYS" />
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.halignment="CENTER" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
        <Button mnemonicParsing="false" onAction="#pressButton" prefHeight="Infinity" prefWidth="Infinity" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" onAction="#newGame" prefHeight="Infinity" prefWidth="Infinity" text="New Game" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    </children>
</GridPane>
